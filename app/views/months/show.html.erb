<div id="calendar-wrap">
            <header>
                <h1>
                  <%= month_header(@month) %>
                </h1>
            </header>
            <div id="calendar">
                <ul class="weekdays">
                    <li>Sunday</li>
                    <li>Monday</li>
                    <li>Tuesday</li>
                    <li>Wednesday</li>
                    <li>Thursday</li>
                    <li>Friday</li>
                    <li>Saturday</li>
                </ul>

            <%= render partial: 'months/days', locals: {month: @month} %>

            </div><!-- /. calendar -->
        </div><!-- /. wrap -->
